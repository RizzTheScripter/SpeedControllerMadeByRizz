local Players = game:GetService("Players") local TweenService = game:GetService("TweenService") local player = Players.LocalPlayer local character = player.Character or player.CharacterAdded:Wait() local humanoid = character:WaitForChild("Humanoid")

local playerGui = player:WaitForChild("PlayerGui") local screenGui = Instance.new("ScreenGui", playerGui) screenGui.Name = "SpeedControlGui" screenGui.ResetOnSpawn = false

--[[==================== GUI LOADING ====================]]-- local loadingFrame = Instance.new("Frame", screenGui) loadingFrame.Size = UDim2.new(0, 300, 0, 150) loadingFrame.Position = UDim2.new(0.5, -150, 0.5, -75) loadingFrame.BackgroundColor3 = Color3.fromRGB(20, 20, 20) loadingFrame.BorderSizePixel = 0 loadingFrame.Active = true loadingFrame.Draggable = true

local cornerLoad = Instance.new("UICorner", loadingFrame) cornerLoad.CornerRadius = UDim.new(0, 10)

local madeByLabel = Instance.new("TextLabel", loadingFrame) madeByLabel.Size = UDim2.new(1, 0, 0, 30) madeByLabel.Position = UDim2.new(0, 0, 0, 10) madeByLabel.BackgroundTransparency = 1 madeByLabel.Text = "MadeByRizz" madeByLabel.Font = Enum.Font.GothamBold madeByLabel.TextColor3 = Color3.new(1, 1, 1) madeByLabel.TextSize = 18

local loadingText = Instance.new("TextLabel", loadingFrame) loadingText.Size = UDim2.new(1, 0, 0, 30) loadingText.Position = UDim2.new(0, 0, 0, 60) loadingText.BackgroundTransparency = 1 loadingText.Text = "Loading: 1%" loadingText.Font = Enum.Font.Gotham loadingText.TextColor3 = Color3.new(1, 1, 1) loadingText.TextSize = 16

local skipBtn = Instance.new("TextButton", loadingFrame) skipBtn.Size = UDim2.new(0, 100, 0, 30) skipBtn.Position = UDim2.new(0.5, -50, 1, -40) skipBtn.Text = "Skip" skipBtn.Visible = false skipBtn.BackgroundColor3 = Color3.fromRGB(255, 170, 0) skipBtn.TextColor3 = Color3.new(1, 1, 1) skipBtn.Font = Enum.Font.GothamBold skipBtn.TextSize = 14

local loadingDone = false local currentLoad = 1 local skipPressed = false

-- Loading Animation local function startLoading() while currentLoad <= 100 and not skipPressed do loadingText.Text = "Loading: " .. currentLoad .. "%" if currentLoad == 50 then skipBtn.Visible = true end currentLoad += 1 task.wait(0.05) end if skipPressed then loadingText.Text = "Loading: 100%" end loadingDone = true task.wait(0.5) loadingFrame:Destroy() mainFrame.Visible = true restoreBtn.Visible = true end

task.spawn(startLoading)

skipBtn.MouseButton1Click:Connect(function() skipPressed = true end)

--[[==================== SPEED CONTROLLER GUI ====================]]-- local mainFrame = Instance.new("Frame", screenGui) mainFrame.Size = UDim2.new(0, 280, 0, 200) mainFrame.Position = UDim2.new(0.5, -140, 0.5, -100) mainFrame.BackgroundColor3 = Color3.fromRGB(20, 20, 20) mainFrame.BorderSizePixel = 0 mainFrame.Active = true mainFrame.Draggable = true mainFrame.Visible = false

local uiCorner = Instance.new("UICorner", mainFrame) uiCorner.CornerRadius = UDim.new(0, 10)

local title = Instance.new("TextLabel", mainFrame) title.Size = UDim2.new(1, 0, 0, 30) title.BackgroundTransparency = 1 title.Text = "Speed Controller âš¡" title.Font = Enum.Font.GothamBold title.TextColor3 = Color3.new(1, 1, 1) title.TextSize = 16

local speedLabel = Instance.new("TextLabel", mainFrame) speedLabel.Position = UDim2.new(0, 10, 0, 40) speedLabel.Size = UDim2.new(0, 100, 0, 25) speedLabel.BackgroundTransparency = 1 speedLabel.Text = "Speed:" speedLabel.Font = Enum.Font.Gotham speedLabel.TextColor3 = Color3.new(1, 1, 1) speedLabel.TextSize = 14 speedLabel.TextXAlignment = Enum.TextXAlignment.Left

local speedBox = Instance.new("TextBox", mainFrame) speedBox.Position = UDim2.new(0, 100, 0, 40) speedBox.Size = UDim2.new(0, 150, 0, 25) speedBox.BackgroundColor3 = Color3.fromRGB(40, 40, 40) speedBox.Text = "16" speedBox.Font = Enum.Font.Gotham speedBox.TextColor3 = Color3.new(1, 1, 1) speedBox.TextSize = 14 speedBox.ClearTextOnFocus = false

local toggle = Instance.new("TextButton", mainFrame) toggle.Position = UDim2.new(0, 10, 0, 80) toggle.Size = UDim2.new(0, 260, 0, 35) toggle.BackgroundColor3 = Color3.fromRGB(0, 170, 255) toggle.Text = "Speed ON" toggle.Font = Enum.Font.GothamBold toggle.TextColor3 = Color3.new(1, 1, 1) toggle.TextSize = 14

local minimizeBtn = Instance.new("TextButton", mainFrame) minimizeBtn.Position = UDim2.new(1, -35, 0, 0) minimizeBtn.Size = UDim2.new(0, 35, 0, 30) minimizeBtn.Text = "_" minimizeBtn.TextSize = 22 minimizeBtn.BackgroundColor3 = Color3.fromRGB(50, 50, 50) minimizeBtn.TextColor3 = Color3.new(1, 1, 1) minimizeBtn.Font = Enum.Font.GothamBold

local madeBy = Instance.new("TextLabel", mainFrame) madeBy.Size = UDim2.new(1, 0, 0, 20) madeBy.Position = UDim2.new(0, 0, 1, -20) madeBy.Text = "MadeByRizz" madeBy.BackgroundTransparency = 1 madeBy.Font = Enum.Font.Gotham madeBy.TextColor3 = Color3.new(1, 1, 1) madeBy.TextSize = 12

local warnLabel = Instance.new("TextLabel", mainFrame) warnLabel.Size = UDim2.new(1, 0, 0, 20) warnLabel.Position = UDim2.new(0, 0, 1, -40) warnLabel.BackgroundTransparency = 1 warnLabel.Text = "" warnLabel.Font = Enum.Font.GothamBold warnLabel.TextColor3 = Color3.fromRGB(255, 255, 0) warnLabel.TextSize = 14

local restoreBtn = Instance.new("TextButton", screenGui) restoreBtn.Position = UDim2.new(0, 10, 1, -45) restoreBtn.Size = UDim2.new(0, 130, 0, 35) restoreBtn.Text = "Open Speed GUI" restoreBtn.BackgroundColor3 = Color3.fromRGB(0, 150, 255) restoreBtn.TextColor3 = Color3.new(1, 1, 1) restoreBtn.TextSize = 14 restoreBtn.Font = Enum.Font.GothamBold restoreBtn.Visible = false

-- Logic local isSpeedOn = true local defaultSpeed = humanoid.WalkSpeed

toggle.MouseButton

